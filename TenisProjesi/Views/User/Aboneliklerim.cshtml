@model IEnumerable<TenisProjesi.Abonelikler>

@{
    ViewBag.Title = "Aboneliklerim";
}

<h2>Aboneliklerim</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Kort_Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.User_Id)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Kort_Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.User_Id)
        </td>
    </tr>
}

</table>
